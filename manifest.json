{
    "name": "QuestTrader",
    "version": "1.0",
    "description": "Chrome extension to update stock prices",
    "permissions": ["storage", "declarativeContent", "alarms"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/questrade.png",
            "32": "images/questrade.png",
            "64": "images/questrade.png",
            "128": "images/questrade.png"
          }
    },
    "icons" : {
        "16" : "images/questrade.png",
        "32" : "images/questrade.png",
        "64" : "images/questrade.png",
        "128" : "images/questrade.png"
    },
    "manifest_version": 2
}